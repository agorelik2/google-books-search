  "main": "server.js",
  "dependencies": {
    "axios": "^0.18.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.3",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "jquery": "^3.5.1",
    "mongoose": "^5.3.16",
    "react": "^16.13.1",
